{
  "name": "@c360/app",
  "version": "4.0.3",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "start-proxy": "lcp --proxyUrl http://one-view-api.dev.zm.private --port 8088 --proxyPartial ''",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:preview": "yarn test:coverage && npx vite preview --outDir ./coverage --open",
    "test:once": "npm run test -- --watchAll=false --maxWorkers=50%",
    "test:ci": "vitest --run --silent --coverage --reporter=json --outputFile=report.json",
    "prettify": "prettier ./src --write",
    "lint": "eslint ./src --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "codegen": "graphql-codegen --require dotenv/config  --config codegen.ts"
  },
  "engines": {
    "node": "16.14",
    "npm": "^8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "5.10.16",
    "@mui/lab": "^5.0.0-alpha.67",
    "@mui/material": "5.10.17",
    "@types/compose-function": "^0.0.30",
    "@types/react-test-renderer": "^18.0.0",
    "axios": "^1.3.4",
    "body-scroll-lock": "^4.0.0-beta.0",
    "compose-function": "^3.0.3",
    "dayjs": "1.11.2",
    "graphql": "16.5.0",
    "graphql-ws": "^5.9.1",
    "jwt-decode": "^3.1.2",
    "lodash": "4.17.21",
    "mixpanel-browser": "2.45.0",
    "notistack": "^2.0.8",
    "numeral": "2.0.6",
    "object-hash": "3.0.0",
    "prop-types": "^15.7.2",
    "query-string": "7.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "6.1.0",
    "react-hook-form": "7.41.1",
    "react-intersection-observer": "9.1.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "^5.0.1",
    "react-shepherd": "4.1.0",
    "source-map-explorer": "^2.5.2",
    "subscriptions-transport-ws": "0.11.0",
    "tss-react": "^3.7.0",
    "validator": "13.7.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/client-preset": "^1.1.1",
    "@graphql-codegen/near-operation-file-preset": "^2.4.4",
    "@graphql-codegen/schema-ast": "^2.5.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.4",
    "@hookform/devtools": "^4.3.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/lodash": "^4.14.182",
    "@types/lodash.template": "^4.5.1",
    "@types/mixpanel-browser": "^2.38.0",
    "@types/node": "^16.11.25",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.1.8",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
